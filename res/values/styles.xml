<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    
    
    <style name="Theme.Material.Light" parent="android:Theme.Material.Light" />

    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.Material.Light">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:colorPrimaryDark">#00796D</item>
        <item name="android:colorPrimary">#009788</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:navigationBarColor">#00796D</item>
        <item name="android:colorAccent">#00796D</item>
        <item name="android:backgroundTint">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
    </style>

    
</resources>
